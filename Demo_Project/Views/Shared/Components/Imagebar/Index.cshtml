@model IEnumerable<Demo_Project.Models.Category>

<div class="container-fluid pt-5">
    <div class="row px-xl-5 pb-3">



        @foreach (var item in Model)
        {
            <div class="col-lg-4 col-md-6 pb-1">
                <div class="cat-item d-flex flex-column border mb-4" style="padding: 30px;">
                    <p class="text-right">15 Products</p>
                    <a href="/Products/ProductsByCate/?categoryId=@item.CategoryId" class="cat-img position-relative overflow-hidden mb-3">
                        <img class="img-fluid" src="@item.CategoryPhoto" alt="">
                    </a>
                    <h5 class="font-weight-semi-bold m-0">@item.CategoryName</h5>
                </div>
            </div>
        }




    </div>
    <!--Start of Tawk.to Script-->
    <script type="text/javascript">
        var Tawk_API = Tawk_API || {}, Tawk_LoadStart = new Date();
        (function () {
            var s1 = document.createElement("script"), s0 = document.getElementsByTagName("script")[0];
            s1.async = true;
            s1.src = 'https://embed.tawk.to/6575c38d70c9f2407f7e093c/1hha07o9j';
            s1.charset = 'UTF-8';
            s1.setAttribute('crossorigin', '*');
            s0.parentNode.insertBefore(s1, s0);
